{
  "name": "super-lean-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass src/sass/main.scss css/style.css -w",
    "compile:sass": "node-sass src/sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "devserver": "live-server",
    "one:won": "npm-run-all --parallel devserver watch:sass",
    "start": "webpack-dev-server --config ./webpack.config.js",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production"
  },
  "keywords": [],
  "author": "Joey Robinson <joeyrobinsondev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "react-hot-loader": "^3.1.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.4.1",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.10",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2"
  }
}
