{
  "name": "super-lean-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass src/sass/main.scss css/style.css -w",
    "compile:sass": "node-sass src/sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "devserver": "live-server",
    "one:won": "npm-run-all --parallel devserver watch:sass",
    "start": "webpack-dev-server --config ./webpack.config.js",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production"
  },
  "keywords": [],
  "author": "Joey Robinson <joeyrobinsondev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "react-hot-loader": "^4.9.0",
    "webpack": "^4.33.0",
    "webpack-dev-server": "^3.6.0"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  }
}
